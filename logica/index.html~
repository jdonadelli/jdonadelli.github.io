  function showorhide(enla , id){
            var ele  = document.getElementById(id);    

            
            if(ele.style.display == "block") {
                ele.style.display = "none";
		enla.innerHTML = "[+info]";
            }
	    else {
		ele.style.display = "block";
		enla.innerHTML = "[-]";
            }
  }

  function showorhide2(enla , id){
            var ele  = document.getElementById(id);    

            
            if(ele.style.display == "block") {
                ele.style.display = "none";
		enla.innerHTML = "[normas]";
            }
	    else {
		ele.style.display = "block";
		enla.innerHTML = "[-]";
            }
  }
  
</script>

